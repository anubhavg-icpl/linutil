{
  "package": {
    "productName": "Linutil Desktop",
    "version": "1.0.0"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "../dist",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": false,
        "show": true,
        "hide": true,
        "close": true,
        "minimize": true,
        "maximize": true,
        "setFullscreen": true,
        "setFocus": true,
        "center": true
      }
    },
    "bundle": {
      "active": true,
      "category": "Utility",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [],
      "identifier": "com.christitus.linutil-desktop",
      "longDescription": "A comprehensive collection of Linux system utilities and tools with a modern desktop interface.",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Linux System Utility Collection",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "label": "main",
        "height": 800,
        "resizable": true,
        "title": "Linutil Desktop",
        "width": 1200,
        "center": true,
        "visible": true,
        "decorations": true,
        "url": "index.html"
      }
    ]
  }
}